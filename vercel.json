{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.ts"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb_uri",
        "MONGODB_DATABASE": "@mongodb_database"
    }
}