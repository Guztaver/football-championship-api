{
    "version": 2,
    "bunVersion": "1.x",
    "functions": {
        "api/index.ts": {
            "runtime": "bun@1"
        }
    },
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.ts"
        }
    ]
}